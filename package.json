{
  "name": "starter-node",
  "version": "1.0.0",
  "description": "starter node project",
  "main": "dist/cluster.js",
  "author": "Svyat Broodd",
  "license": "ISC",
  "scripts": {
    "low-start": "node dist/cluster.js",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/cluster.js",
    "build-ts": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/cluster.js",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test --watchAll",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/cluster.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "dotenv": "^6.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.3",
    "express-flash": "0.0.2",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "mongoose": "^5.7.7",
    "multer": "^1.4.2",
    "validator": "^12.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/compression": "^1.0.1",
    "@types/dotenv": "^6.1.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.1",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lusca": "^1.6.1",
    "@types/mongoose": "^5.5.29",
    "@types/multer": "^1.3.10",
    "@types/node": "^10.12.7",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^10.11.3",
    "concurrently": "^4.0.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.3",
    "npm": "^6.13.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
